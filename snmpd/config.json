{
  "name": "SNMP Server (raw config)",
  "version": "1.0.11",
  "slug": "snmpdraw",
  "description": "SNMP v2c server for monitoring",
  "url": "https://github.com/indigoparadox/hassio-snmpd",
  "arch": ["armhf", "armv7", "amd64", "i386"],
  "startup": "services",
  "hassio_role": "manager",
  "privileged": ["SYS_ADMIN"],
  "full_access": true,
  "host_dbus": true,
  "map": ["config:ro"],
  "boot": "auto",
  "init": false,
  "homeassistant_api": true,
  "hassio_api": true,
  "schema": {
    "args": "str"
  },
  "options": {
    "args": "-f -c /config/snmpd.conf"
  },
  "ports": {
    "8161/udp": 8161
  }
}
